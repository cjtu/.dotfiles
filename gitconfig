[user]
	name = Christian Tai Udovicic
	email = cj.taiudovicic@gmail.com
[alias]
	st = status
	su = submodule update
	co = checkout
	cm = commit -m
	ca = commit --amend --no-edit
	fo = fetch origin
	df = diff-tree --no-commit-id --name-only -r HEAD
	lp = log -5 --graph --pretty=format:\"%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset\"
	ra = rebase --abort
	rc = rebase --continue
	rd = rebase origin/dev
	re = rebase origin/experimental
	br = branch

[push]
	default = simple
[core]
	editor = vim

[color]
	ui = auto
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
